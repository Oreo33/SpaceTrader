  <children>
    <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="49.0" layoutY="50.0" onMouseClicked="#goToPlanet" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../../Images/planet1.png" />
