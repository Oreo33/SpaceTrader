    </ImageView>
    <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToSolarSystem" text="Back" />
  </children>
  <stylesheets>
    <URL value="@general.css" />
  </stylesheets>
</AnchorPane>
